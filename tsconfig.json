{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "include":[
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "gulpclass.ts",
        "dist",
        ".vscode"
    ]
}
